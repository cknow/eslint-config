{
  "name": "eslint-config-clicknow",
  "version": "0.0.0-semantically-released",
  "description": "ESLint shareable config for ClickNow",
  "main": "index.js",
  "files": [
    "index.js",
    "browser.js",
    "config",
    "plugins",
    "rules"
  ],
  "scripts": {
    "commit": "git-cz",
    "watch": "nodemon --exec npm t",
    "lint": "eslint index.js browser.js esnext.js config/ plugins/ rules/ test/",
    "test": "npm run cover",
    "postest": "npm run check-coverage",
    "cover": "nyc --reporter lcov --reporter text ava",
    "check-coverage": "nyc check-coverage --global 100",
    "report-coverage": "cat ./coverage/lcov.info | codecov",
    "semantic-release": "semantic-release pre && npm publish && semantic-release post"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/cknow/eslint-config-clicknow"
  },
  "keywords": [
    "eslint",
    "eslintconfig",
    "code",
    "quality",
    "style",
    "lint",
    "linter",
    "jscs",
    "jshint",
    "jslint",
    "eslint",
    "validate",
    "code style",
    "standard",
    "strict",
    "check",
    "checker",
    "verify",
    "enforce",
    "hint",
    "babel",
    "angular",
    "jquery",
    "mocha",
    "jasmine",
    "ava",
    "protractor"
  ],
  "author": {
    "name": "Ricardo Gobbo de Souza",
    "email": "ricardogobbosouza@yahoo.com.br",
    "role": "Developer"
  },
  "license": "MIT",
  "homepage": "https://github.com/cknow/eslint-config-clicknow",
  "bugs": {
    "url": "https://github.com/cknow/eslint-config-clicknow/issues",
    "email": "ricardo@clicknow.com.br"
  },
  "preferGlobal": false,
  "engines": {
    "node": ">=4"
  },
  "devDependencies": {
    "ava": "^0.15.2",
    "babel-eslint": "^6.1.0",
    "codecov": "^1.0.1",
    "commitizen": "^2.8.2",
    "cz-conventional-changelog": "^1.1.6",
    "eslint": "^3.0.0",
    "eslint-plugin-angular": "^1.3.0",
    "eslint-plugin-ava": "^2.5.0",
    "eslint-plugin-babel": "^3.3.0",
    "eslint-plugin-jasmine": "^1.8.1",
    "eslint-plugin-jquery": "^1.0.0",
    "eslint-plugin-mocha": "^4.0.0",
    "eslint-plugin-node": "^2.0.0",
    "eslint-plugin-protractor": "^1.11.2",
    "ghooks": "^1.3.2",
    "lodash": "^4.13.1",
    "nodemon": "^1.9.2",
    "nyc": "^6.6.1",
    "semantic-release": "^4.3.5",
    "temp-write": "^2.1.0"
  },
  "peerDependencies": {
    "eslint": ">=2"
  },
  "config": {
    "ghooks": {
      "pre-commit": "npm run lint",
      "pre-push": "npm t"
    },
    "commitizen": {
      "path": "node_modules/cz-conventional-changelog"
    }
  }
}
