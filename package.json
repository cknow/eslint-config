{
  "name": "@cknow/eslint-config",
  "version": "0.0.0-semantically-released",
  "description": "ESLint shareable config",
  "keywords": [
    "eslint",
    "eslint-config",
    "eslint-rules",
    "javascript",
    "config",
    "code",
    "quality",
    "style",
    "lint",
    "linter",
    "jscs",
    "jshint",
    "jslint",
    "eslint",
    "validate",
    "code style",
    "standard",
    "strict",
    "check",
    "checker",
    "verify",
    "enforce",
    "hint",
    "angular",
    "jquery",
    "mocha",
    "jasmine",
    "ava",
    "protractor",
    "vue"
  ],
  "homepage": "https://github.com/cknow/eslint-config",
  "bugs": {
    "url": "https://github.com/cknow/eslint-confi/issues",
    "email": "ricardo@clicknow.com.br"
  },
  "license": "MIT",
  "author": {
    "name": "Ricardo Gobbo de Souza",
    "email": "ricardogobbosouza@yahoo.com.br",
    "role": "Developer"
  },
  "files": [
    "index.js",
    "browser.js",
    "esnext.js",
    "config",
    "plugins",
    "rules"
  ],
  "main": "index.js",
  "repository": {
    "type": "git",
    "url": "https://github.com/cknow/eslint-config.git"
  },
  "scripts": {
    "check-coverage": "nyc check-coverage --global 100",
    "commit": "commit",
    "coverage": "nyc --reporter lcov --reporter text ava",
    "lint": "eslint index.js browser.js esnext.js config/ plugins/ rules/ test/",
    "postcoverage": "npm run check-coverage",
    "precoverage": "rimraf .nyc_output coverage",
    "pretest": "npm run lint",
    "report-coverage": "cat ./coverage/lcov.info | coveralls",
    "test": "npm run coverage",
    "watch": "ava --watch"
  },
  "commitlint": {
    "extends": [
      "@commitlint/config-conventional"
    ]
  },
  "devDependencies": {
    "@commitlint/cli": "^8.0.0",
    "@commitlint/config-conventional": "^8.0.0",
    "@commitlint/prompt-cli": "^8.0.0",
    "ava": "^2.0.0",
    "babel-eslint": "^10.0.1",
    "coveralls": "^3.0.4",
    "eslint": "^5.16.0",
    "eslint-plugin-angular": "^4.0.1",
    "eslint-plugin-ava": "^7.0.0",
    "eslint-plugin-jasmine": "^2.10.1",
    "eslint-plugin-jest": "^22.6.4",
    "eslint-plugin-jquery": "^1.5.0",
    "eslint-plugin-mocha": "^5.3.0",
    "eslint-plugin-node": "^9.1.0",
    "eslint-plugin-promise": "^4.1.1",
    "eslint-plugin-protractor": "^1.41.1",
    "eslint-plugin-vue": "^5.2.2",
    "husky": "^2.4.0",
    "lodash": "^4.17.11",
    "nyc": "^14.1.1",
    "rimraf": "^2.6.3",
    "temp-write": "^4.0.0",
    "vue-eslint-parser": "^6.0.4"
  },
  "peerDependencies": {
    "eslint": ">=5.16.0"
  },
  "engines": {
    "node": ">=8"
  },
  "private": false,
  "publishConfig": {
    "access": "public"
  }
}
