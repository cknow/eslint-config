{
  "name": "@cknow/eslint-config",
  "version": "0.0.0-semantically-released",
  "description": "ESLint shareable config",
  "keywords": [
    "eslint",
    "eslint-config",
    "eslint-rules",
    "javascript",
    "config",
    "code",
    "quality",
    "style",
    "lint",
    "linter",
    "jscs",
    "jshint",
    "jslint",
    "eslint",
    "validate",
    "code style",
    "standard",
    "strict",
    "check",
    "checker",
    "verify",
    "enforce",
    "hint",
    "angular",
    "jquery",
    "mocha",
    "jasmine",
    "ava",
    "protractor"
  ],
  "homepage": "https://github.com/cknow/eslint-config",
  "bugs": {
    "url": "https://github.com/cknow/eslint-confi/issues",
    "email": "ricardo@clicknow.com.br"
  },
  "license": "MIT",
  "author": {
    "name": "Ricardo Gobbo de Souza",
    "email": "ricardogobbosouza@yahoo.com.br",
    "role": "Developer"
  },
  "files": [
    "index.js",
    "browser.js",
    "esnext.js",
    "config",
    "plugins",
    "rules"
  ],
  "main": "index.js",
  "repository": {
    "type": "git",
    "url": "https://github.com/cknow/eslint-config"
  },
  "scripts": {
    "check-coverage": "nyc check-coverage --global 100",
    "commit": "git-cz",
    "commitmsg": "validate-commit-msg",
    "cover": "nyc --reporter lcov --reporter text ava",
    "lint": "eslint index.js browser.js esnext.js config/ plugins/ rules/ test/",
    "posttest": "npm run check-coverage",
    "precommit": "npm run lint",
    "precover": "rimraf .nyc_output coverage",
    "prepush": "npm t",
    "pretest": "npm run lint",
    "report-coverage": "cat ./coverage/lcov.info | coveralls",
    "semantic-release": "semantic-release pre && npm publish && semantic-release post",
    "test": "npm run cover",
    "watch": "nodemon --exec npm t"
  },
  "config": {
    "commitizen": {
      "path": "node_modules/cz-conventional-changelog"
    }
  },
  "devDependencies": {
    "ava": "^0.22.0",
    "babel-core": "^6.26.0",
    "babel-preset-env": "^1.6.0",
    "commitizen": "^2.9.6",
    "coveralls": "^3.0.0",
    "cz-conventional-changelog": "^2.0.0",
    "eslint": "^4.9.0",
    "eslint-plugin-angular": "^3.1.1",
    "eslint-plugin-ava": "^4.2.2",
    "eslint-plugin-jasmine": "^2.8.4",
    "eslint-plugin-jquery": "^1.2.1",
    "eslint-plugin-mocha": "^4.11.0",
    "eslint-plugin-node": "^5.2.0",
    "eslint-plugin-protractor": "^1.40.0",
    "husky": "^0.14.3",
    "lodash": "^4.17.4",
    "nodemon": "^1.12.1",
    "nyc": "^11.2.1",
    "rimraf": "^2.6.2",
    "semantic-release": "^8.0.3",
    "temp-write": "^3.3.0",
    "validate-commit-msg": "^2.14.0"
  },
  "peerDependencies": {
    "eslint": ">=4.9.0"
  },
  "engines": {
    "node": ">=4",
    "npm": ">=3"
  },
  "preferGlobal": false,
  "private": false
}
